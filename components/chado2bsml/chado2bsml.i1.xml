<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
	<name>chado2bsml</name>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>Create the FASTA output directory</name>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;FASTA_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/chado2bsml</executable>
	    <name>chado2bsml</name>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>
		<key>--database_type</key>
		<value>$;DATABASE_TYPE$;</value>
	    </param>
	    <param>
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>$;PASSWORD$;</value>
	    </param>
	    <param>
		<key>--username</key>
		<value>$;USERNAME$;</value>
	    </param>
	    <param>
		<key>--asmbl</key>
		<value>$;ASMBL$;</value>
	    </param>
	    <param>
		<key>--bsml_file_name</key>
		<value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;ASMBL$;.bsml</value>
	    </param>
	    <param>
		<key>--log4perl</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/chado2bsml.pl.$;ASMBL_ID$;.log</value>
	    </param>
	    <param>
		<key>--debug</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--fastadir</key>
		<value>$;FASTA_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/chado2bsml.$;ASMBL_ID$;.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/chado2bsml.$;ASMBL_ID$;.stderr</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>DTD validation</name>
	    <executable>$;BIN_DIR$;/dtdValid.pl</executable>
	    <arg>-d $;BSML_DTD$; $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;ASMBL_ID$;.bsml</arg>
	</command>
    </commandSet>
</commandSetRoot>
